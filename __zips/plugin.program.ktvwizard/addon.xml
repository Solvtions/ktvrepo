<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.program.ktvwizard" name="KriTiK TV Wizard" version="0.1.10" provider-name="KriTiK TV">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">KriTiK TV Wizard</summary>
    <description lang="en">KriTiK TV Wizard allows you to get access to the latest KriTiK TV Custom Builds and much more..!!</description>
    <platform>all</platform>
